BEGIN
	BLOCK "080404B0"
	BEGIN
	VALUE "CompanyName",		"shuax"
	VALUE "FileDescription",	"基于Lua脚本的剪贴板增强软件"
	VALUE "LegalCopyright",	"Copyright (C) 2013-2014 www.shuax.com"
	VALUE "ProductName",		"LuaClipboard"
	VALUE "FileVersion",		VERSION_STR
	VALUE "ProductVersion",	VERSION_STR
	END
END
BLOCK "VarFileInfo"
BEGIN
	VALUE "Translation", 0x804, 1200
END
END
