#include <windows.h>
#include "resource.h"

1 MANIFEST "./Res/LuaClip.manifest"

IDI_APPICON ICON DISCARDABLE "./Res/LuaClip.ico"
IDI_APPICON_SMALL ICON DISCARDABLE "./Res/LuaClip-Small.ico"
